<div>
  <%= render'shared/errors', obj: @review %>
  <%= form_with model: @review, local: false do |f| %>
    <div class="container" id="review_add">
      <p><%= f.label :body %></p>
      <p><%= f.text_field :user_review, class: 'review_id' %></p>
    </div>
      <%= f.hidden_field :product_id %>
    <div>
      <%= f.submit %>
    </div>
  <% end %>
</div>
